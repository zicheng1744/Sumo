<?xml version="1.0" encoding="UTF-8"?>
<routes>
    <vType id="car" vClass="passenger" speedDev="0.2" accel="2.6" decel="4.5" sigma="0.5" />
    
    <!-- 各方向车流 -->
    <flow id="flow_NS" type="car" begin="0" end="1000" period="3" 
          from="north_in" to="south_in" departLane="random" departSpeed="max"/>
    <flow id="flow_SN" type="car" begin="0" end="1000" period="4" 
          from="south_in" to="north_in" departLane="random" departSpeed="max"/>
    <flow id="flow_EW" type="car" begin="0" end="1000" period="5" 
          from="east_in" to="west_in" departLane="random" departSpeed="max"/>
    <flow id="flow_WE" type="car" begin="0" end="1000" period="6" 
          from="west_in" to="east_in" departLane="random" departSpeed="max"/>
</routes>