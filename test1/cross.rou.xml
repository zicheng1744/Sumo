<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
    <!-- 定义车辆类型 -->
    <vType id="car" accel="2.6" decel="4.5" sigma="0.5" length="5" minGap="2.5" maxSpeed="16.67" color="1,1,0"/>
    
    <!-- 定义路线 -->
    <route id="route0" edges="north_in center south_out"/>
    <route id="route1" edges="south_in center north_out"/>
    <route id="route2" edges="east_in center west_out"/>
    <route id="route3" edges="west_in center east_out"/>
    
    <!-- 目标车辆 -->
    <vehicle id="veh0" type="car" route="route0" depart="0" color="1,0,0"/>
    
    <!-- 其他车辆流 -->
    <flow id="flow0" begin="0" end="3600" vehsPerHour="100" from="north_in" to="south_out" color="0,1,0"/>
    <flow id="flow1" begin="0" end="3600" vehsPerHour="100" from="south_in" to="north_out" color="0,1,0"/>
    <flow id="flow2" begin="0" end="3600" vehsPerHour="100" from="east_in" to="west_out" color="0,1,0"/>
    <flow id="flow3" begin="0" end="3600" vehsPerHour="100" from="west_in" to="east_out" color="0,1,0"/>
</routes>